{
   "ts-node":{
      "require":[
         "tsconfig-paths/register"
      ]
   },
   "compilerOptions":{
      "baseUrl":"./",
      "paths":{
         "@/*":[
            "src/*"
         ],
         "@lib/*":[
            "lib/*"
         ]
      },
      "target":"es2016",
      "module":"commonjs",
      "outDir":"./dist",
      "esModuleInterop":true,
      "forceConsistentCasingInFileNames":true,
      "strict":true,
      "skipLibCheck":true
   }
}